{
    "extends": ["airbnb"],
    "env": {
        "jest": true
    },
    "rules": {
        "indent": ["error", 4],
        "no-return-await": 0,
        "no-else-return": 0,
        "consistent-return": 0,
        "object-shorthand": 0,
        "no-console": 0,
        "arrow-body-style": 0,
        "arrow-parens": 0,
        "implicit-arrow-linebreak": 0,
        "prefer-promise-reject-errors": 0,
        "import/prefer-default-export": 0,
        "eol-last": 0,
        "object-property-newline": 0,
        "max-len": ["error", { "code": 120 }]
    }
}